package ast_utils;

public class ASTNode {
	public Integer ndIdx;
	public Integer ndParentIdx;
	public Integer[] ndChildrenIdx;
	public String ndType;
	public String ndValue;
	
	public ASTNode(Integer ndIdx, Integer parentInd, Integer[] childInd, String type) {
		this.ndIdx = ndIdx;
		this.parentInd = parentInd;
		this.childInd = childInd;
		this.type = type;
		this.value = "";
	}
	
	
	
}
