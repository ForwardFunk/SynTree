package ast_utils;

public class ASTNode {
	public Integer ind;
	public Integer parentInd;
	public Integer[] childInd;
	public String type;
	public String value;
	
	public ASTNode(Integer ind, Integer parentInd, Integer[] childInd, String type) {
		this.ind = ind;
		this.parentInd = parentInd;
		this.childInd = childInd;
		this.type = type;
		this.value = "";
	}
	
	
	
}
